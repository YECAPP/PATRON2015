PROCEDURE GENNUMPDA
PARAMETERS MES, AÑO, TIPO
STORE 1 TO NUMSUG 
DIMENSION NUMSPDAS(1)
NUMSPDAS(1)=0
SELECT NUMERO FROM ("..\C2.DBF") WHERE TP=TIPO AND YEAR(FECHA)=AÑO AND MONTH(FECHA)=MES GROUP BY 1 INTO ARRAY NUMSPDAS
FOR EACH NUMPDA IN NUMSPDAS
	IF NUMPDA=NUMSUG THEN 
		NUMSUG=NUMSUG+1
		LOOP
	ELSE
		EXIT
	ENDIF
ENDFOR
RETURN NUMSUG
ENDPROC
